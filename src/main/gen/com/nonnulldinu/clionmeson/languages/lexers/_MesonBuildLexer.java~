            } 
            // fall through
          case 60: break;
          case 11: 
            { return BRACE_END;
            } 
            // fall through
          case 61: break;
          case 12: 
            { return CURLY_BRACE_BEGIN;
            } 
            // fall through
          case 62: break;
          case 13: 
            { return CURLY_BRACE_END;
            } 
            // fall through
          case 63: break;
          case 14: 
            { return PAREN_BEGIN;
            } 
            // fall through
          case 64: break;
          case 15: 
            { return PAREN_END;
            } 
            // fall through
          case 65: break;
          case 16: 
            { return COMMA;
            } 
            // fall through
          case 66: break;
          case 17: 
            { return ATR_OP;
            } 
            // fall through
          case 67: break;
          case 18: 
            { return COMP_OP4;
            } 
            // fall through
          case 68: break;
          case 19: 
            { return COMP_OP5;
            } 
            // fall through
          case 69: break;
          case 20: 
            { return MOD_OP;
            } 
            // fall through
          case 70: break;
          case 21: 
            { return PLUS_OP;
            } 
            // fall through
          case 71: break;
          case 22: 
            { return MINUS_OP;
            } 
            // fall through
          case 72: break;
          case 23: 
            { return LANG_TOKEN_QMARK;
            } 
            // fall through
          case 73: break;
          case 24: 
            { return LANG_TOKEN_COLON;
            } 
            // fall through
          case 74: break;
          case 25: 
            { return STRSIMPLE;
            } 
            // fall through
          case 75: break;
          case 26: 
            { return SINGLE_LINE_COMMENT;
            } 
            // fall through
          case 76: break;
          case 27: 
            { return COMP_ATR_OP2;
            } 
            // fall through
          case 77: break;
          case 28: 
            { return COMP_ATR_OP1;
            } 
            // fall through
          case 78: break;
          case 29: 
            { return IN_OP;
            } 
            // fall through
          case 79: break;
          case 30: 
            { return LANG_TOKEN_IF;
            } 
            // fall through
          case 80: break;
          case 31: 
            { return COMP_OP1;
            } 
            // fall through
          case 81: break;
          case 32: 
            { return COMP_OP2;
            } 
            // fall through
          case 82: break;
          case 33: 
            { return COMP_OP3;
            } 
            // fall through
          case 83: break;
          case 34: 
            { return COMP_OP6;
            } 
            // fall through
          case 84: break;
          case 35: 
            { return COMP_ATR_OP3;
            } 
            // fall through
          case 85: break;
          case 36: 
            { return COMP_ATR_OP4;
            } 
            // fall through
          case 86: break;
          case 37: 
            { return COMP_ATR_OP5;
            } 
            // fall through
          case 87: break;
          case 38: 
            { return BINNUM;
            } 
            // fall through
          case 88: break;
          case 39: 
            { return HEXNUM;
            } 
            // fall through
          case 89: break;
          case 40: 
            { return NOT_OP;
            } 
            // fall through
          case 90: break;
          case 41: 
            { return MULTILINE_COMMENT;
            } 
            // fall through
          case 91: break;
          case 42: 
            { return BOOL_LITERAL_TRUE;
            } 
            // fall through
          case 92: break;
          case 43: 
            { return LANG_TOKEN_ELSE_IF;
            } 
            // fall through
          case 93: break;
          case 44: 
            { return LANG_TOKEN_ELSE;
            } 
            // fall through
          case 94: break;
          case 45: 
            { return LANG_TOKEN_JUMP_BREAK;
            } 
            // fall through
          case 95: break;
          case 46: 
            { return LANG_TOKEN_ENDIF;
            } 
            // fall through
          case 96: break;
          case 47: 
            { return BOOL_LITERAL_FALSE;
            } 
            // fall through
          case 97: break;
          case 48: 
            { return LANG_TOKEN_FOREACH;
            } 
            // fall through
          case 98: break;
          case 49: 
            { return LANG_TOKEN_JUMP_CONTINUE;
            } 
            // fall through
          case 99: break;
          case 50: 
            { return LANG_TOKEN_FOREACH_END;
            } 
            // fall through
          case 100: break;
          default:
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
